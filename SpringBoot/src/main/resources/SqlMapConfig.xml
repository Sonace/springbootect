<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

    <typeAliases>
        <typeAlias alias="User" type="com.SpringBootect.son.model.User" />
        <typeAlias alias="Role" type="com.SpringBootect.son.model.Role" />
        <typeAlias alias="Address" type="com.SpringBootect.son.model.Address" />
    </typeAliases>

    <mappers>
        <mapper resource="com/SpringBootect/son/sql/role.xml" />
        <mapper resource="com/SpringBootect/son/sql/user.xml" />
        <mapper resource="com/SpringBootect/son/sql/address.xml" />
    </mappers>

</configuration>